    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "js-joda": "^1.11.0",
    "jsonwebtoken": "^9.0.1",
    "multer-s3": "^3.0.1",
    "nest-winston": "^1.9.3",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-kakao": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ts-jest": "29.1.0",
    "jest": "29.5.0",
    "supertest": "^6.1.3"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-ioredis": "^2.0.3",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.13",
    "@types/node": "18.16.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "prisma": "^4.16.2",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>",
      "@shared/(.*)": "<rootDir>/shared/$1",
      "@user/(.*)": "<rootDir>/user/$1",
      "@auth/(.*)": "<rootDir>/auth/$1",
      "@admin/(.*)": "<rootDir>/admin/$1",
      "@redis/(.*)": "<rootDir>/redis/$1",
      "@cache/(.*)": "<rootDir>/cache/$1",
      "@badge/(.*)": "<rootDir>/badge/$1",
      "@point/(.*)": "<rootDir>/point/$1",
      "@task/(.*)": "<rootDir>/task/$1"
    }
  }
